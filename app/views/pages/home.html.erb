<% if user_signed_in? %>
	<div class="jumbotron">
		<h1> You're logged in!</h1>
	</div>
<% else %>
	<div class="jumbotron">
		<h1> Welcome to One Month rails!</h1>
		<p>
			You've found the home page for the <%= link_to "One Month Rails", "http://onemonthrails.com" %> application.
		</p>

		<p>
			<%= link_to "Sign Up Now!", new_user_registration_path, class: "btn btn-primary btn-lg"%>
		</p>	
	</div>
<% end %>